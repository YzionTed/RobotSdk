<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearLayout1"
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:layout_alignParentLeft="true"
              android:layout_alignParentTop="true"
              android:orientation="vertical">

    <Button
        android:id="@+id/ButtonSetup"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Setup"/>

    <Button
        android:id="@+id/ButtonConsole"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Console"/>

    <Button
        android:id="@+id/ButtonLoopback"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Loopback"/>

    <Button
        android:id="@+id/Button01010101"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Send 01010101"/>

    <Button
        android:id="@+id/ButtonAbout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="About"/>

    <Button
        android:id="@+id/ButtonQuit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Quit"/>

    <Button
        android:id="@+id/go"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="go"/>

    <Button
        android:id="@+id/back"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="back"/>

    <Button
        android:id="@+id/goleft"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="goleft"/>

    <Button
        android:id="@+id/goright"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="goright"/>

    <Button
        android:id="@+id/openProjector"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="openProjector"/>

    <Button
        android:id="@+id/closeProjector"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="closeProjector"/>


</LinearLayout>